<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <Appenders>
        <Console name="logConsola" target="SYSTEM_OUT">
         <PatternLayout
                pattern="  %d{yyyy-MM-dd HH:MM:ss}   %l %p  %m%n" />
        </Console>
      
    </Appenders>
    <Loggers>
        <Root level="trace">
           <AppenderRef ref="logConsola"/>
        </Root>   
    </Loggers>
</Configuration>